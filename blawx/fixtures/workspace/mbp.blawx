      blawx_attribute_nlg(unable_to_use_vehicle,vo,"it is","that for claim","the vehicle
      could not be used").

      #pred unable_to_use_vehicle(Y,X) :: ''it is @(X) that for claim @(Y) the vehicle
      could not be used''.

      #pred according_to(R,unable_to_use_vehicle(Y,X)) :: ''according to @(R), it
      is @(X) that for claim @(Y) the vehicle could not be used''.

      #pred legally_holds(_,unable_to_use_vehicle(Y,X)) :: ''it legally holds that
      it is @(X) that for claim @(Y) the vehicle could not be used''.

      opposes(unable_to_use_vehicle(X,true),unable_to_use_vehicle(X,false)).

      opposes(unable_to_use_vehicle(X,false),unable_to_use_vehicle(X,true)).


      blawx_attribute(claim,cause,claim_cause).

      blawx_attribute_nlg(cause,ov,"in claim","the cause was","").

      #pred cause(X,Y) :: ''in claim @(X) the cause was @(Y)''.

      #pred according_to(R,cause(X,Y)) :: ''according to @(R), in claim @(X) the cause
      was @(Y)''.

      #pred legally_holds(_,cause(X,Y)) :: ''it legally holds that in claim @(X) the
      cause was @(Y)''.


      blawx_category(claim_cause).

      #pred claim_cause(X) :: ''@(X) is a claim_cause''.

      #pred according_to(R,claim_cause(X)) :: ''according to @(R), @(X) is a claim_cause''.

      #pred legally_holds(_,claim_cause(X)) :: ''it legally holds that @(X) is a claim_cause''.

      claim_cause(mechanical_breakdown).

      claim_cause(vandalism).

      claim_cause(fire).

      claim_cause(theft_or_attempted_theft).

      claim_cause(flat_tyre).

      claim_cause(flat_battery).

      claim_cause(accident).

      claim_cause(out_of_fuel).

      claim_cause(wrong_fuel).

      claim_cause(keys_missing_broken_etc).

      blawx_category(breakdown).

      #pred breakdown(X) :: ''@(X) is a breakdown''.

      #pred according_to(R,breakdown(X)) :: ''according to @(R), @(X) is a breakdown''.

      #pred legally_holds(_,breakdown(X)) :: ''it legally holds that @(X) is a breakdown''.


      according_to(sec_1__subsec_10_section,  breakdown(Claim)) :-

      claim(Claim),

      claim_cause(Cause),

      cause(Claim,Cause).'
