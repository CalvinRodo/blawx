    scasp_encoding: 'blawx_category(policy).

      #pred policy(X) :: ''@(X) is a policy''.

      #pred according_to(R,policy(X)) :: ''according to @(R), @(X) is a policy''.

      #pred legally_holds(_,policy(X)) :: ''it legally holds that @(X) is a policy''.

      blawx_attribute(policy,excess_selected,boolean).

      blawx_attribute_nlg(excess_selected,vo,"it is","that policy","includes excess
      provision").

      #pred excess_selected(Y,X) :: ''it is @(X) that policy @(Y) includes excess
      provision''.

      #pred according_to(R,excess_selected(Y,X)) :: ''according to @(R), it is @(X)
      that policy @(Y) includes excess provision''.

      #pred legally_holds(_,excess_selected(Y,X)) :: ''it legally holds that it is
      @(X) that policy @(Y) includes excess provision''.

      opposes(excess_selected(X,true),excess_selected(X,false)).

      opposes(excess_selected(X,false),excess_selected(X,true)).


      blawx_attribute(policy,excess_amount,number).

      blawx_attribute_nlg(excess_amount,ov,"under policy","the excess amount charged
      is","").

      #pred excess_amount(X,Y) :: ''under policy @(X) the excess amount charged is
      @(Y)''.

      #pred according_to(R,excess_amount(X,Y)) :: ''according to @(R), under policy
      @(X) the excess amount charged is @(Y)''.

      #pred legally_holds(_,excess_amount(X,Y)) :: ''it legally holds that under policy
      @(X) the excess amount charged is @(Y)''.


      blawx_category(claim).

      #pred claim(X) :: ''@(X) is a claim''.

      #pred according_to(R,claim(X)) :: ''according to @(R), @(X) is a claim''.

      #pred legally_holds(_,claim(X)) :: ''it legally holds that @(X) is a claim''.

      blawx_attribute(claim,policy,policy).

      blawx_attribute_nlg(policy,ov,"claim","is made under policy","").

      #pred policy(X,Y) :: ''claim @(X) is made under policy @(Y)''.

      #pred according_to(R,policy(X,Y)) :: ''according to @(R), claim @(X) is made
      under policy @(Y)''.

      #pred legally_holds(_,policy(X,Y)) :: ''it legally holds that claim @(X) is
      made under policy @(Y)''.


      blawx_attribute(claim,valid,boolean).

      blawx_attribute_nlg(valid,vo,"it is","that claim","is valid").

      #pred valid(Y,X) :: ''it is @(X) that claim @(Y) is valid''.

      #pred according_to(R,valid(Y,X)) :: ''according to @(R), it is @(X) that claim
      @(Y) is valid''.

      #pred legally_holds(_,valid(Y,X)) :: ''it legally holds that it is @(X) that
      claim @(Y) is valid''.

      opposes(valid(X,true),valid(X,false)).

      opposes(valid(X,false),valid(X,true)).


      blawx_attribute(claim,excess_amount,number).

      blawx_attribute_nlg(excess_amount,ov,"for claim","the excess amount owing is","").

      #pred excess_amount(X,Y) :: ''for claim @(X) the excess amount owing is @(Y)''.

      #pred according_to(R,excess_amount(X,Y)) :: ''according to @(R), for claim @(X)
      the excess amount owing is @(Y)''.

      #pred legally_holds(_,excess_amount(X,Y)) :: ''it legally holds that for claim
      @(X) the excess amount owing is @(Y)''.


      blawx_attribute(claim,excess_amount_paid_prior_to_service,boolean).

      blawx_attribute_nlg(excess_amount_paid_prior_to_service,vo,"it is","that the
      excess for claim","has been paid prior to service").

      #pred excess_amount_paid_prior_to_service(Y,X) :: ''it is @(X) that the excess
      for claim @(Y) has been paid prior to service''.

      #pred according_to(R,excess_amount_paid_prior_to_service(Y,X)) :: ''according
      to @(R), it is @(X) that the excess for claim @(Y) has been paid prior to service''.

      #pred legally_holds(_,excess_amount_paid_prior_to_service(Y,X)) :: ''it legally
      holds that it is @(X) that the excess for claim @(Y) has been paid prior to
      service''.

      opposes(excess_amount_paid_prior_to_service(X,true),excess_amount_paid_prior_to_service(X,false)).

      opposes(excess_amount_paid_prior_to_service(X,false),excess_amount_paid_prior_to_service(X,true)).


      blawx_attribute(claim,service_eligible,boolean).

      blawx_attribute_nlg(service_eligible,vo,"it is","that the claim","is eligible
      for service").

      #pred service_eligible(Y,X) :: ''it is @(X) that the claim @(Y) is eligible
      for service''.

      #pred according_to(R,service_eligible(Y,X)) :: ''according to @(R), it is @(X)
      that the claim @(Y) is eligible for service''.

      #pred legally_holds(_,service_eligible(Y,X)) :: ''it legally holds that it is
      @(X) that the claim @(Y) is eligible for service''.

      opposes(service_eligible(X,true),service_eligible(X,false)).

      opposes(service_eligible(X,false),service_eligible(X,true)).



      according_to(sec_1__subsec_2_section,  excess_amount(Claim,Amount)) :-

      policy(Policy),

      claim(Claim),

      policy(Claim,Policy),

      excess_selected(Policy,true),

      excess_amount(Policy,Amount).


      according_to(sec_1__subsec_2_section,  service_eligible(Claim,true)) :-

      policy(Policy),

      claim(Claim),

      policy(Claim,Policy),

      excess_selected(Policy,true),

      according_to(sec_1__subsec_2_section,  excess_amount(Claim,Amount)),

      excess_amount_paid_prior_to_service(Claim,true).


      according_to(sec_1__subsec_2_section,  service_eligible(Claim,true)) :-

      policy(Policy),

      claim(Claim),

      policy(Claim,Policy),

      not excess_selected(Policy,true).'
