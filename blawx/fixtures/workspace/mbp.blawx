    scasp_encoding: 'blawx_category(misfuelling).

      #pred misfuelling(X) :: ''@(X) is a misfuelling''.

      #pred according_to(R,misfuelling(X)) :: ''according to @(R), @(X) is a misfuelling''.

      #pred legally_holds(_,misfuelling(X)) :: ''it legally holds that @(X) is a misfuelling''.

      blawx_attribute(claim,cause_accidental,boolean).

      blawx_attribute_nlg(cause_accidental,vo,"it is","that the cause of claim","was
      accidental").

      #pred cause_accidental(Y,X) :: ''it is @(X) that the cause of claim @(Y) was
      accidental''.

      #pred according_to(R,cause_accidental(Y,X)) :: ''according to @(R), it is @(X)
      that the cause of claim @(Y) was accidental''.

      #pred legally_holds(_,cause_accidental(Y,X)) :: ''it legally holds that it is
      @(X) that the cause of claim @(Y) was accidental''.

      opposes(cause_accidental(X,true),cause_accidental(X,false)).

      opposes(cause_accidental(X,false),cause_accidental(X,true)).



      according_to(sec_1__subsec_14_section,  misfuelling(Claim)) :-

      claim(Claim),

      cause(Claim,wrong_fuel),

      cause_accidental(Claim,true).'
