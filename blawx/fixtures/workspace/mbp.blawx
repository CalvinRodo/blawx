    scasp_encoding: 'blawx_category(vehicle_type).

      #pred vehicle_type(X) :: ''@(X) is a vehicle_type''.

      #pred according_to(R,vehicle_type(X)) :: ''according to @(R), @(X) is a vehicle_type''.

      #pred legally_holds(_,vehicle_type(X)) :: ''it legally holds that @(X) is a
      vehicle_type''.

      blawx_attribute(vehicle,vehicle_type,vehicle_type).

      blawx_attribute_nlg(vehicle_type,ov,"vehicle","is of type","").

      #pred vehicle_type(X,Y) :: ''vehicle @(X) is of type @(Y)''.

      #pred according_to(R,vehicle_type(X,Y)) :: ''according to @(R), vehicle @(X)
      is of type @(Y)''.

      #pred legally_holds(_,vehicle_type(X,Y)) :: ''it legally holds that vehicle
      @(X) is of type @(Y)''.


      blawx_attribute(vehicle,private,boolean).

      blawx_attribute_nlg(private,vo,"it is","that","is a private vehicle").

      #pred private(Y,X) :: ''it is @(X) that @(Y) is a private vehicle''.

      #pred according_to(R,private(Y,X)) :: ''according to @(R), it is @(X) that @(Y)
      is a private vehicle''.

      #pred legally_holds(_,private(Y,X)) :: ''it legally holds that it is @(X) that
      @(Y) is a private vehicle''.

      opposes(private(X,true),private(X,false)).

      opposes(private(X,false),private(X,true)).


      blawx_attribute(vehicle,length,number).

      blawx_attribute_nlg(length,ov,"the length of","is","").

      #pred length(X,Y) :: ''the length of @(X) is @(Y)''.

      #pred according_to(R,length(X,Y)) :: ''according to @(R), the length of @(X)
      is @(Y)''.

      #pred legally_holds(_,length(X,Y)) :: ''it legally holds that the length of
      @(X) is @(Y)''.


      blawx_attribute(vehicle,weight,number).

      blawx_attribute_nlg(weight,ov,"the weight of","is","").

      #pred weight(X,Y) :: ''the weight of @(X) is @(Y)''.

      #pred according_to(R,weight(X,Y)) :: ''according to @(R), the weight of @(X)
      is @(Y)''.

