    scasp_encoding: 'blawx_attribute(policy,policy_holder,party).

      blawx_attribute_nlg(policy_holder,vo,"party","is the policy holder for","").

      #pred policy_holder(Y,X) :: ''party @(X) is the policy holder for @(Y)''.

      #pred according_to(R,policy_holder(Y,X)) :: ''according to @(R), party @(X)
      is the policy holder for @(Y)''.

      #pred legally_holds(_,policy_holder(Y,X)) :: ''it legally holds that party @(X)
      is the policy holder for @(Y)''.


      blawx_attribute(claim,involved_party,party).

      blawx_attribute_nlg(involved_party,vo,"party","is involved in claim","").

      #pred involved_party(Y,X) :: ''party @(X) is involved in claim @(Y)''.

      #pred according_to(R,involved_party(Y,X)) :: ''according to @(R), party @(X)
      is involved in claim @(Y)''.

      #pred legally_holds(_,involved_party(Y,X)) :: ''it legally holds that party
      @(X) is involved in claim @(Y)''.


      blawx_attribute(claim,insured_party,party).

      blawx_attribute_nlg(insured_party,vo,"party","is an insured party under claim","").

      #pred insured_party(Y,X) :: ''party @(X) is an insured party under claim @(Y)''.

      #pred according_to(R,insured_party(Y,X)) :: ''according to @(R), party @(X)
      is an insured party under claim @(Y)''.

      #pred legally_holds(_,insured_party(Y,X)) :: ''it legally holds that party @(X)
      is an insured party under claim @(Y)''.


      blawx_attribute(claim,driver,party).

      blawx_attribute_nlg(driver,vo,"party","was driving the vehicle wrt claim","").

      #pred driver(Y,X) :: ''party @(X) was driving the vehicle wrt claim @(Y)''.

      #pred according_to(R,driver(Y,X)) :: ''according to @(R), party @(X) was driving
      the vehicle wrt claim @(Y)''.

      #pred legally_holds(_,driver(Y,X)) :: ''it legally holds that party @(X) was
      driving the vehicle wrt claim @(Y)''.


      blawx_attribute(claim,passenger,party).

      blawx_attribute_nlg(passenger,vo,"party","was a passenger in the vehicle wrt
      claim","").

      #pred passenger(Y,X) :: ''party @(X) was a passenger in the vehicle wrt claim
      @(Y)''.

      #pred according_to(R,passenger(Y,X)) :: ''according to @(R), party @(X) was
      a passenger in the vehicle wrt claim @(Y)''.

      #pred legally_holds(_,passenger(Y,X)) :: ''it legally holds that party @(X)
      was a passenger in the vehicle wrt claim @(Y)''.


      blawx_attribute(claim,number_requesting_assistance,number).

      blawx_attribute_nlg(number_requesting_assistance,ov,"wrt to claim","","individuals
      are seeking assistance").

      #pred number_requesting_assistance(X,Y) :: ''wrt to claim @(X)  @(Y) individuals
      are seeking assistance''.

      #pred according_to(R,number_requesting_assistance(X,Y)) :: ''according to @(R),
      wrt to claim @(X)  @(Y) individuals are seeking assistance''.

      #pred legally_holds(_,number_requesting_assistance(X,Y)) :: ''it legally holds
      that wrt to claim @(X)  @(Y) individuals are seeking assistance''.



      according_to(sec_1__subsec_4_section,  insured_party(Claim,Party)) :-

      policy(Policy),

      claim(Claim),

      party(Party),

      policy(Claim,Policy),

      involved_party(Claim,Party),

      policy_holder(Policy,Party).


      according_to(sec_1__subsec_4_section,  insured_party(Claim,Party)) :-

      policy(Policy),

      claim(Claim),

      party(Party),

      policy(Claim,Policy),

      involved_party(Claim,Party),

      driver(Claim,Party).


      according_to(sec_1__subsec_4_section,  insured_party(Claim,Party)) :-

      policy(Policy),

      claim(Claim),

      party(Party),

      policy(Claim,Policy),

      involved_party(Claim,Party),

      passenger(Claim,Party).


      according_to(sec_1__subsec_4_section,  valid(Claim,false)) :-

      policy(Policy),

      claim(Claim),

      policy(Claim,Policy),

      number_requesting_assistance(Claim,Number),

      Number > 7.'
