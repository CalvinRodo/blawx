      #pred vehicle(X) :: ''@(X) is a vehicle''.

      #pred according_to(R,vehicle(X)) :: ''according to @(R), @(X) is a vehicle''.

      #pred legally_holds(_,vehicle(X)) :: ''it legally holds that @(X) is a vehicle''.

      blawx_attribute(policy,vehicles_shown_on_schedule,vehicle).

      blawx_attribute_nlg(vehicles_shown_on_schedule,vo,"vehicle","is shown on the
      schedule of policy","").

      #pred vehicles_shown_on_schedule(Y,X) :: ''vehicle @(X) is shown on the schedule
      of policy @(Y)''.

      #pred according_to(R,vehicles_shown_on_schedule(Y,X)) :: ''according to @(R),
      vehicle @(X) is shown on the schedule of policy @(Y)''.

      #pred legally_holds(_,vehicles_shown_on_schedule(Y,X)) :: ''it legally holds
      that vehicle @(X) is shown on the schedule of policy @(Y)''.


      blawx_attribute(claim,relevant_vehicle,vehicle).

      blawx_attribute_nlg(relevant_vehicle,ov,"claim","is wrt vehicle","").

      #pred relevant_vehicle(X,Y) :: ''claim @(X) is wrt vehicle @(Y)''.

      #pred according_to(R,relevant_vehicle(X,Y)) :: ''according to @(R), claim @(X)
      is wrt vehicle @(Y)''.

      #pred legally_holds(_,relevant_vehicle(X,Y)) :: ''it legally holds that claim
      @(X) is wrt vehicle @(Y)''.


      blawx_attribute(claim,covered,boolean).

      blawx_attribute_nlg(covered,vo,"it is","that claim","is covered").

      #pred covered(Y,X) :: ''it is @(X) that claim @(Y) is covered''.

      #pred according_to(R,covered(Y,X)) :: ''according to @(R), it is @(X) that claim
      @(Y) is covered''.

      #pred legally_holds(_,covered(Y,X)) :: ''it legally holds that it is @(X) that
      claim @(Y) is covered''.

      opposes(covered(X,true),covered(X,false)).

      opposes(covered(X,false),covered(X,true)).



      according_to(sec_1__subsec_3_section,  covered(Claim,false)) :-

      policy(Policy),

      claim(Claim),

      vehicle(Vehicle),

      policy(Claim,Policy),

      relevant_vehicle(Claim,Vehicle),

      not vehicles_shown_on_schedule(Policy,Vehicle).


